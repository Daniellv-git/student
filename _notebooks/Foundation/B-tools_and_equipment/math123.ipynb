    "// Homework Problem #4: Check if y is divisible by 5\n",
    "\n",
    "let y = 20; // You can change this value to test different numbers\n",
    "\n",
    "let remainder = y % 5;\n",
    "\n",
    "console.log(y + \" % 5 = \" + remainder);\n",
    "\n",
    "if (y % 5 === 0) {\n",
    "    console.log(y + \" is divisible by 5\");\n",
    "} else {\n",
    "    console.log(y + \" is NOT divisible by 5\");\n",
    "}"
