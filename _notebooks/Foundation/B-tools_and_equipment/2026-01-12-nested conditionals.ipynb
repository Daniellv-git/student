for (let number = 1; number <= 50; number++) {
    if (number % 2 === 0) {
        console.log(number + " is even");
        
        if (number % 10 === 0) {
            console.log(number + " is divisible by 10");
            
            if (number % 50 === 0) {
                console.log(number + " is divisible by 50");
            } else {
                console.log(number + " is not divisible by 50");
            }
        } else {
            console.log(number + " is not divisible by 10");
        }
        
        if (number % 3 === 0) {
            console.log(number + " is also divisible by 3");
        }
    } else {
        console.log(number + " is odd");
        
        if (number % 5 === 0) {
            console.log(number + " is divisible by 5");
            
            if (number % 25 === 0) {
                console.log(number + " is divisible by 25");
            } else {
                console.log(number + " is not divisible by 25");
            }
        } else {
            console.log(number + " is not divisible by 5");
        }
        
        if (number % 3 === 0) {
            console.log(number + " is also divisible by 3");
        } else {
            console.log(number + " is not divisible by 3");
        }
    }
    
    console.log(number + " is divisible by 1");
}
